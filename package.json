{
  "name": "temporal-worker-typescript",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "tsc --build",
    "build.watch": "tsc --build --watch",
    "lint": "eslint .",
    "start": "ts-node src/worker.ts",
    "start.watch": "nodemon src/worker.ts",
    "workflow": "ts-node src/client.ts",
    "format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "test": "mocha --exit --require ts-node/register --require source-map-support/register src/mocha/*.test.ts",
    "preinstall": "npx only-allow pnpm",
    "start.prod": "node dist/worker.js"
  },
  "nodemonConfig": {
    "execMap": {
      "ts": "ts-node"
    },
    "ext": "ts",
    "watch": [
      "src"
    ]
  },
  "packageManager": "pnpm@9.13.2",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.693.0",
    "@temporalio/activity": "^1.11.3",
    "@temporalio/client": "^1.11.3",
    "@temporalio/worker": "^1.11.3",
    "@temporalio/workflow": "^1.11.3",
    "axios": "^1.7.7",
    "bottleneck": "^2.19.5",
    "dotenv": "^16.4.5",
    "https-proxy-agent": "^7.0.5",
    "ioredis": "^5.4.1",
    "joi": "^17.13.3",
    "neo4j-driver": "^5.26.0",
    "pako": "^2.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@temporalio/testing": "^1.11.3",
    "@tsconfig/node16": "^16.1.3",
    "@types/eslint__js": "^8.42.3",
    "@types/mocha": "~10.0.9",
    "@types/neo4j": "^2.0.6",
    "@types/node": "^22.9.0",
    "@types/pako": "^2.0.3",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.12.0",
    "mocha": "~10.8.2",
    "nodemon": "^3.1.7",
    "prettier": "^3.3.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.14.0"
  }
}